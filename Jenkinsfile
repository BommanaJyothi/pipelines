pipeline{
  agent any
  stages {
    stage('build'){
      steps{
        echo 'build stage....'
        echo 'hello world.'
      }
    }
  stage('compile'){
      steps{
        echo 'compile stage....'
      }
    }
  stage('install'){
      steps{
        echo 'install stage....'
      }
    }
  }
}
